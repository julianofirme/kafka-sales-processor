{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/orders/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/orders/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/orders",
  "orders": {
    "type": "application",
    "root": "apps/orders",
    "entryFile": "main",
    "sourceRoot": "apps/orders/src",
    "compilerOptions": {
      "tsConfigPath": "apps/orders/tsconfig.app.json"
    }
  }
}